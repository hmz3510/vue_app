          show(){console.log(\"执行了show方法\")}\r\n        },\r\n        beforeCreate(){\r\n            //生命周期第一个函数,实例被创建'之前',会执行它\r\n            //注意:执行函数时，data和methods还没有初始化创建，也就不能调用里面的数据和方法\r\n            console.log(this.msg);//undefiend\r\n            this.show()//报错\r\n        },\r\n        created(){\r\n            //生命周期第二个函数，实例被创建'之后'\r\n            //注意：执行函数时datahemethods已经初始化创建完成，可用调用模型数据和方法\r\n            console.log(this.msg) //脚手架\r\n            this.show()  //成功\r\n        },\r\n        beforeMount(){\r\n            //生命周期第三个函数,模板在内存中已经编译完成，尚未把模板渲染到页面中，还没生成DOM树 也就不能执行DOM操作\r\n           // console.log(document.getElementById(\"h3\"));\r\n           // console.log(h3.innerText)\r\n        },\r\n        mounted(){\r\n            //生命周期第四个函数,模板在内存中已经编译完成,把模板写入页面中 页可用执行DOM操作\r\n            console.log(document.getElementById(\"h3\").innerText)\r\n        },\r\n        beforeUpdate(){\r\n            //生命周期第五个\r\n            //当执行beforeUpdate 页面中显示数据旧\r\n            //此时  data数据为新\r\n            console.log(\"5内存中Data变量\" + this.msg)\r\n            var h3 = document.getElementById(\"h3\")\r\n            console.log(\"5模板中数据\" + h3.innerHTML);\r\n        },\r\n        updated(){\r\n            //生命周期第六个函数\r\n            console.log(\"6内存中Data变量\" + this.msg)\r\n            var h3 = document.getElementById(\"h3\")\r\n            console.log(\"6模板中数据\" + h3.innerHTML);\r\n        }\r\n    }\r\n</script>\r\n\r\n\r\n<style scoped>\r\n    \r\n</style>"],"sourceRoot":"src\\components"}]}