ntent-inner\">\r\n\t\t\t\t\t\t<p>商品货号:{{info.pid}}</p>\r\n                        <p>商品颜色:土豪金</p>\r\n                        <p>商品运费:0</p>    \r\n\t\t\t\t\t</div>\r\n\t\t\t\t</div>\r\n\t\t</div>\r\n    </div>\r\n</template>\r\n<script>\r\n/*引入轮播图组件 */\r\nimport carousel from \"../sub/carousel.vue\"\r\n//引入muijs库\r\nimport {Toast} from \"mint-ui\"\r\n\r\n    export default {\r\n        data(){\r\n            return{\r\n                id:this.$route.params.id,\r\n                list:[],\r\n                num:1,\r\n                info:[]\r\n            }\r\n        },\r\n       \r\n        methods:{\r\n            getImg(){\r\n                this.$http.get(\"imagelist\").then(result=>{\r\n                    //console.log(result.body)\r\n                    this.list= result.body\r\n                    console.log(this.list)\r\n                    \r\n                })\r\n            },\r\n            getAdd(){\r\n                if(this.num<999)\r\n                this.num++;\r\n            },\r\n            getDel(){\r\n                if(this.num>1){\r\n                    this.num--;\r\n                }else{\r\n                    this.num=1;\r\n                }\r\n            },\r\n            getInfo(){\r\n                this.$http.get(\"goodsinfo?id=\"+this.id).then(result=>{\r\n                //console.log(result);\r\n                this.info=result.body;\r\n                //console.log(this.info)\r\n            })\r\n            },\r\n            addCart(){\r\n                this.$http.get(\"addCart?pid=\"+this.id+\"&count=\"+this.num).then(result=>{\r\n                    if(result.body.code==1){\r\n                        this.$store.commit(\"increment\",this.num);\r\n                        Toast(result.body.msg)\r\n                    }else{\r\n                        Toast(result.body.msg)\r\n                    }\r\n                })\r\n            }\r\n        },\r\n        created(){\r\n            this.getImg();\r\n            this.getInfo()\r\n        },\r\n        //返回模拟数据[id,title,now,old,货号] 携带商品id\r\n        components:{\r\n            \"carousel\":carousel\r\n        }\r\n    }\r\n</script>\r\n<style>\r\n   .app-carousel .mint-swipe{\r\n        height:200px;\r\n        width:100%;\r\n    }\r\n    .app-carousel .mint-swipe .mint-swipe-item img{\r\n        width:100%;\r\n    }\r\n</style>\r\n"],"sourceRoot":"src\\components\\goods"}]}